    "express": "^4.18.2",
    "octokit": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.1",
    "nodemon": "^2.0.15",
    "prettier": "3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
