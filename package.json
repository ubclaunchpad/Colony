{
  "name": "rocket-bot",
  "version": "0.1.0",
  "description": "LP discord bot",
  "main": "build/app.js",
  "type": "module",
  "engines": {
    "node": "^18.x"
  },
  "scripts": {
    "start": "node build/server.js",
    "register": "node build/deploy-commands.js",
